generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Page {
  id Int @id @default(autoincrement())
  slug String @unique
  metaTitle String @map("meta_title")
  metaDescription String? @map("meta_description")
  @@map("pages")
}